set xrange [3:10]
set yrange [0:1.1]
set terminal pngcairo enhanced color lw 2 size 800,600 font 'Arial-Bold'
set output "Prob-R1.png"
set xlabel "Time (sec)" font "Verdana"
set ylabel "Mark Probability" font "Verdana"
set key outside;
set key right top;
plot "Prob-R-1.plotme" title "R1" with lines lw 1.5

