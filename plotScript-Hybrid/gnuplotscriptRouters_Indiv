set terminal pngcairo enhanced color lw 2 size 800,600 font 'Arial-Bold'
set yrange [0:110]
set xrange [3:10]
set output "TP-Router-Percent_R1.png"
set xlabel "Time (sec)"
set ylabel "Link Utilization (%)"
set key outside;
set key right top;
plot "TP-T1.plotme" using ($1):($2/.1) title "R1" with lines lw 1.5

